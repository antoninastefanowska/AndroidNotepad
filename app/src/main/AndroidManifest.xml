<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tonia.notatnik">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_notepad"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_notepad"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NotatnikActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".EdytujActivity" />
        <activity android:name=".SzukajActivity" />
        <activity android:name=".BlokadaActivity" />
        <activity android:name=".SzczegolyActivity" />
    </application>

</manifest>